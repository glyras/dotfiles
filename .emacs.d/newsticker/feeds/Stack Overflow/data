printf(\"Could not open database.\\n\");
exit(1);
}
const char* zSql = \"INSERT INTO abc VALUES (?)\";
ret = sqlite3_prepare_v2(sites[DB, zSql, strlen(zSql), &amp;s, NULL);
if (SQLITE_OK != ret) {
printf(\"Could not compile statement '%s', got %d.\\n\", zSql, ret);
exit(1);
}
</code></pre>
