(("KDE.news" nil "http://dot.kde.org" (19641 34294 239361) feed 0 nil nil ((title nil "KDE.news") (link nil "http://dot.kde.org") (description nil) (language nil "en") (item nil (title nil "Amarok Celebrates Success, Announces Roktober Fundraiser") (link nil "http://dot.kde.org/2010/10/15/amarok-celebrates-success-announces-roktober-fundraiser") (description nil "<div class=\"taxonomy-images\"><a href=\"/taxonomy/term/13\" class=\"taxonomy-image-links\"><img src=\"http://dot.kde.org/sites/dot.kde.org/files/category_pictures/user-identity.png\" alt=\"Applications\" title=\"Applications\"  width=\"48\" height=\"48\" /></a></div><div style=\"float: right; padding: 1ex; margin: 1ex; border: solid grey 1px;\">
<img src=\"http://dot.kde.org/sites/dot.kde.org/files/rocktober-wee.jpg\" width=\"350\" height=\"282\" /><br />
