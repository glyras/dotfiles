<br />
<br />
<pre>#!/bin/bash
# findnearest
# Usage: findnearest latitude longitude
# Ex.  findnearest  17.976227 -66.111016
lat=$1
long=$2
result=$(lynx -dump \"http://maps.google.com/maps/geo?output=csv&oe=utf-8&ll=$lat,$long\")
echo $result | cut -f3- -d,
</pre>
